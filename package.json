{
  "name": "pico-markdown-parser",
  "version": "1.0.2",
  "description": "Convert Markdown like syntax to HTML",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "jsnext:main": "dist/index.js",
  "scripts": {
    "build": "NODE_ENV=production BABEL_ENV=production rollup -c",
    "build:watch": "rollup -c -w",
    "test": "jest --watch --env=jsdom"
  },
  "keywords": [
    "Markdown",
    "Parser",
    "Markdown-to-HTML",
    "HTML",
    "text"
  ],
  "author": "rafi993",
  "license": "MIT",
  "dependencies": {
    "arcsecond": "^2.0.4"
  },
  "peerDependencies": {
    "arcsecond": "^2.0.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-node-resolve": "^8.1.0",
    "jest": "^26.1.0",
    "rollup": "^2.21.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-peer-deps-external": "^2.2.3",
    "rollup-plugin-terser": "^6.1.0"
  },
  "jest": {
    "moduleNameMapper": {
      "^src(.*)$": "<rootDir>/src$1"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/cypress/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Rafi993/pico.git"
  },
  "bugs": {
    "url": "https://github.com/Rafi993/pico/issues"
  },
  "homepage": "https://github.com/Rafi993/pico#readme"
}
